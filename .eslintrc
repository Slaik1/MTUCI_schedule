{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-single"],
    "indent": ["warn", 4],
    "comma-dangle": ["warn", "only-multiline"],
    "semi": ["warn", "never"],
    "import/order": ["warn", {
"groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"]
    }]
  }
}